---
import BaseLayout from "../../layouts/BaseLayout.astro";
import { coreLinks } from "../../utils/data";

const baseUrl = import.meta.env.BASE_URL;
---

<BaseLayout tabTitle="CS Core">
  <h1>CS Core</h1>
  <ul>
    {
      coreLinks.map((entry) => (
        <h2>
          <li>
            <a href={`${baseUrl}/core/${entry.url}`.toString()}>
              {entry.title}
            </a>
          </li>
        </h2>
      ))
    }
  </ul>
</BaseLayout>
